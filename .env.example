# BrokerCursor Database Configuration
# Copy this file to .env and fill in your PostgreSQL connection details

# PostgreSQL Connection
DB_HOST=localhost
DB_PORT=5432
DB_NAME=brokercursor
DB_USER=brokercursor_user
DB_PASSWORD=your_secure_password

# Application Settings
APP_ENV=development
LOG_LEVEL=INFO

# File Paths
INBOX_PATH=modules/broker-reports/inbox
ARCHIVE_PATH=modules/broker-reports/archive
PARSED_PATH=modules/broker-reports/parsed

# Processing Settings
MAX_FILE_SIZE_MB=50
SUPPORTED_EXTENSIONS=.html,.txt,.pdf,.md
